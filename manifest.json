{
    "name": "Mr. NoBot",
    "description": "Mr. NoBot",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "hello.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "js": [ "scripts/content.js" ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "https://podrzistudente.org/",
        "https://blokadnedonacije.rs/",
        "https://mrezasolidarnosti.org/"
    ]
}